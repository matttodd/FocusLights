{
    "manifest_version": 2,
    "name": "ProcrastiNOT",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["crackhead.js"]
        }
    ],
    "background": {
        "scripts": ["buttonWrapper.js"]
    },
    "browser_action": {
        "default_icon": "crackhead.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"]
}